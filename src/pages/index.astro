---
import Layout from '../layouts/Layout.astro';
import Services from "../components/sections/services.astro";
import Who from "../components/sections/who.astro";
import Hero from "../components/sections/hero.astro";
import Portfolio from "../components/sections/portfolio.astro";
import Clients from "../components/sections/clients.astro";
import WorkExperience from "../components/sections/workExperience.astro";
import Statistics from "../components/sections/statistics.astro";
import Subscribe from "../components/sections/subscribe.astro";
import Writing from "../components/sections/writing.astro";
import Contact from "../components/sections/contact.astro";
---

<Layout title="Welcome to Astro.">
